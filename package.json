{
  "name": "banka",
  "version": "1.0.0",
  "description": "A lightweight banking support application",
  "main": "app.js",
  "engines": {
    "npm": "6.4.0",
    "node": "8.11.0"
  },
  "scripts": {
    "start": "node build/server.js",
    "postinstall": "node_modules/.bin/rimraf build && babel ./ -d build --ignore node_modules,UI,test",
    "dev": "DEBUG=development nodemon --exec babel-node server.js",
    "lint": "node_modules/.bin/eslint ./",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PreciousTosin/Banka.git"
  },
  "keywords": [
    "banking",
    "online"
  ],
  "author": "Tosin Akinbobola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PreciousTosin/Banka/issues"
  },
  "homepage": "https://github.com/PreciousTosin/Banka#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "bcryptjs": "^2.4.3",
    "debug": "^4.1.1",
    "decimal.js": "^10.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "nyc": "^14.0.0",
    "pg": "^7.10.0",
    "rimraf": "^2.6.3",
    "save-dev": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "babel-plugin-istanbul": "^5.1.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "temp-directory": "./node_modules/coverage/.nyc_output"
  }
}
